{
  "name": "typedoc-plugin-inline-sources",
  "version": "1.0.0",
  "description": "A plugin for TypeDoc that inlines file sources into comments.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "npm run lint && npm run build && npm run test",
    "build": "rm -rf dist && tsc",
    "release": "standard-version",
    "demo": "npm run build && typedoc --tsconfig ./tsconfig.test.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-inline-sources.git"
  },
  "author": "Tom Grey",
  "license": "MIT",
  "engines": {
    "node": ">= 10.8.0"
  },
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-inline-sources/issues"
  },
  "homepage": "https://github.com/tgreyuk/typedoc-plugin-inline-sources#readme",
  "keywords": [
    "typedocplugin",
    "sources",
    "source",
    "code"
  ],
  "peerDependencies": {
    "typedoc": ">=0.20.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "eslint": "^7.27.0",
    "jest": "^27.0.3",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
